- content_for :meta_title, 'Sign In'

%h2 Sign In

= turbo_form resource, url: user_session_path do |f|

  = f.input :email, required: false
  = f.input :password
  = f.input :remember_me, as: :boolean, error: false

  = f.submit 'Sign In'

= render 'users/shared/providers'
= render 'users/shared/links'
