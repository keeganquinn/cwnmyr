{
  "name": "cwnmyr",
  "private": true,
  "scripts": {
    "lint": "eslint app",
    "test": "jest --coverage"
  },
  "jest": {
    "coverageDirectory": "coverage-js",
    "coverageReporters": [
      "cobertura",
      "html",
      "text"
    ],
    "moduleDirectories": [
      "app/assets",
      "node_modules"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "roots": [
      "app/assets"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "jest": true
    },
    "extends": "standard",
    "plugins": [
      "standard",
      "promise"
    ]
  },
  "engines": {
    "node": ">= 8.9.4",
    "yarn": ">= 1.3.2"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@rails/webpacker": "^4.0.7",
    "ahoy.js": "^0.3.4",
    "bootstrap": "^4.3.1",
    "bootstrap-table": "^1.15.2",
    "jquery": "^3.4.1",
    "popper.js": "^1.15.0",
    "rails-ujs": "^5.2.3",
    "turbolinks": "^5.2.0",
    "webpack": "^4.35.2"
  },
  "devDependencies": {
    "eslint": "^6.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-formatter-checkstyle-relative-paths": "^1.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.8.0",
    "jest-google-maps-mock": "^1.0.1",
    "jest-junit": "^6.4.0",
    "webpack-dev-server": "^3.7.2"
  }
}
